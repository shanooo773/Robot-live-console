# Frontend Environment Configuration Template
# Copy this file to .env for local development
# For production, create .env.production with production values

# API Configuration
VITE_API_URL=http://localhost:8000

# For production VPS deployment:
# VITE_API_URL=https://your-domain.com/api
# or
# VITE_API_URL=http://your-vps-ip:8000

# Optional: Enable development features
VITE_ENABLE_DEV_TOOLS=true

# Optional: GitHub OAuth (if implemented)
VITE_GITHUB_CLIENT_ID=

# Optional: Analytics or tracking
VITE_ANALYTICS_ID=